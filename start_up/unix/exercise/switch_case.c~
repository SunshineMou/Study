#include <stdio.h> 

int main() 
{
    int a=1; 
    switch(a) 
    { 
        int b=20; 
        printf("ok!\n");
        case 1: 
            printf("b is %d\n",b); 
            break; 
        default: 
            printf("b is %d\n",b); 
            break; 
    } 
    
    return 0; 
}

