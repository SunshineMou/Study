#include <stdio.h>

#define printd(expr)  printf(#expr " = %s\n", expr)

int global = 0;

int new_printf(char* argv)
{
    printd(argv);
     
    return 0;
}

int new_printf_2(char* argv)
{
    printd(argv);
     
    return 0;
}
