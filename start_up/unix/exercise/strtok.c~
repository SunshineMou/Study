#include <string.h>
#include <stdio.h>

main()
{
    char s[]="Golden,Global,View";
    char *p;

    p = strtok(s, ",");
    while(p != NULL)
    {
        printf("%s\n",p);
        p = strtok(NULL, ",");
    }

    return 0;
}

