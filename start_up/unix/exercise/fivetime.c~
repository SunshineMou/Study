#include <stdio.h>

#define PrintInt(argv) printf("%s : %d\n", #argv, (argv))

int FiveTimes(int a){
    int t; 
    t = (a << 2) + a;

    return t;
}

int main(int argc, char* argv[])
{
    int a = -1, b = 2, c = 3;
    PrintInt(FiveTimes(a));
    PrintInt(FiveTimes(b));
    PrintInt(FiveTimes(c));
     
    return 0;
}

