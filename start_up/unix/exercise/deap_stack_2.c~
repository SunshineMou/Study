#include<stdio.h>

int pass(int a, int b, int c);

int main( ) {
    int x;
    x = 0;
    pass(1, 2, 3);
    x = 1;
    printf("%d",x);
}

int pass(int a, int b, int c) {
    char buffer[16];
    int sum;
    int *ret;
    ret = (int*)(buffer+28);
    (*ret) += 7;
    sum = a + b + c;
    return sum;
}
