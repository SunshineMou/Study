#include <stdio.h>

int main(int argc, char* argv[])
{
    static int a[10], b[10];

    int c[10] = {4, 5, 6};
    int d[10] = {1, 2, 3};

    int *e[]  = {a, b};
    int *f[]  = {c, d};
     
    printf("Success!\n");
    return 0;
}

