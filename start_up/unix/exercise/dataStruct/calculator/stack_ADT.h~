typedef unsigned int data;

int stack_init(data num);
void stack_push(data num);
data stack_pop();
int stack_empty();
int  stack_full();
void stack_release();

int char_stack_init(char num);
void char_stack_push(char num);
char char_stack_pop();
int char_stack_empty();
int  char_stack_full();
void char_stack_release();
